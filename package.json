  "scripts": {
    "dev:demo": "cross-env NODE_ENV=development vue-cli-service serve",
    "dev:docs": "vuepress dev document -c document/.vuepress/config.js",
    "build": "yarn build:lib && yarn build:docs && yarn build:demo",
    "build:lib": "webpack --config build/webpack.build.config.js",
    "build:demo": "cross-env NODE_ENV=production vue-cli-service build",
    "build:docs": "cross-env NODE_ENV=production vuepress build document",
    "build:styles": "cd packages/styles && yarn clean && yarn build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue,.ts,.tsx ./packages",
    "lint:demo": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue,.ts,.tsx ./examples",
    "lint:docs": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue,.md ./document",
    "lint:style": "stylelint \"{packages,examples}/**/*.less\" --syntax --fix less",
    "lint:types": "eslint -c ./.eslintrc.js --fix --ext .ts ./types",
    "new": "plop",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js"
  },
